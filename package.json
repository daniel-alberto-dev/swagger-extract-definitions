{
  "name": "swagger-extract-definitions",
  "version": "1.0.1",
  "description": "Retrieves TypeScript definitions from Swagger json files",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-alberto-dev/swagger-extract-definitions.git"
  },
  "keywords": [
    "extract",
    "definitions",
    "ts",
    "typescript",
    "typings",
    "JSON Schema"
  ],
  "author": "Daniel Alberto <daniel.developer.us@gmail.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/daniel-alberto-dev/swagger-extract-definitions/issues"
  },
  "main": "./dist",
  "homepage": "https://github.com/daniel-alberto-dev/swagger-extract-definitions#readme",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc",
    "prettier": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "fastify": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "pretty-quick": "^4.1.1",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "bin": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  }
}
